{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "CPU",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/CPU_Approach/main.out",
            "args": ["0.01","128","100","100","10","1","12","index"],
            //       freq,   numclass, numtrx, skew, maxtxn,mintxn, numthreads, index
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "make"
        }
    ]
}